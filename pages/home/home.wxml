<view class='flex-r-sa top-types-wrap'>
  <view
    wx:for="{{types}}"
    wx:key="{{index}}"
    class="top-types-item {{currentType == index ? 'active' : ''}}"
    data-index='{{index}}'
    bindtap='tapTypes'
