/* pages/home/home.wxss */

.search-wrap{
  padding: 0 20rpx;
  height: 80rpx;
  background-color: #fff;
}
.search-wrap .search-icon{
  width: 26rpx;
  height: 26rpx;
}
.search-wrap .search-input{
  flex: 1;
  padding: 0 14rpx;
  height: 52rpx;
  border: 1rpx solid #cccccc;
  border-radius: 26rpx;
}
.search-wrap .search-input .input-style{
  margin-left: 10rpx;
  font-size: 24rpx;
}
.search-wrap .search-btn{
  /* flex-shrink: 1; */
  margin-left: 50rpx;
  width: 128rpx;
  height: 54rpx;
  line-height: 54rpx;
  border-radius: 27rpx;
  background-color: #f0f0f0;
}
.search-wrap .search-btn text{
  margin-left: 5rpx;
  font-size: 22rpx;
}

/* 分类 */
.list-wrap{
  background-color: #fff;
}

.list-wrap .list-item{
  margin-bottom: 10rpx;
  height: 360rpx;
}
.list-wrap .list-item image{
  width: 100%;
  height: 100%;
  background-color: pink;  
}